{
  "name": "@eclaire/core",
  "version": "0.0.0",
  "private": true,
  "description": "Core utilities for Eclaire applications - ID generation, encryption, types",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./id": {
      "import": "./dist/id-generator.js",
      "require": "./dist/id-generator.js",
      "types": "./dist/id-generator.d.ts"
    },
    "./encryption": {
      "import": "./dist/encryption.js",
      "require": "./dist/encryption.js",
      "types": "./dist/encryption.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  }
}
