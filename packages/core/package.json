{
  "name": "@eclaire/core",
  "version": "0.0.0",
  "private": true,
  "description": "Core utilities for Eclaire applications - ID generation, encryption, types",
  "license": "MIT",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./id": {
      "development": "./src/id-generator.ts",
      "default": "./dist/id-generator.js"
    },
    "./encryption": {
      "development": "./src/encryption.ts",
      "default": "./dist/encryption.js"
    },
    "./types": {
      "development": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./utils": {
      "development": "./src/utils.ts",
      "default": "./dist/utils.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  }
}
